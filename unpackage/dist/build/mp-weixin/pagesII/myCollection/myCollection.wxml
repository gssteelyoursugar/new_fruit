<view><block wx:if="{{modaishow}}"><view class="container-img"><image class="img-quanguo" src="../../static/images/quanguo.png" mode="widthFix"></image><view class="btnbox"><button class="btn" type="default" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['getUserInfo',['$event']]]]]}}" bindgetuserinfo="__e">去登录</button></view></view></block><block wx:if="{{!modaishow&&likeDatas.length==0}}"><view class="container-img no-likes"><image src="../../static/images/no_like.png" mode="widthFix"></image><text class="color-text">最近没有收藏过商品~</text></view></block><block wx:if="{{!modaishow&&likeDatas.length!==0}}"><view class="tui-tab-rank"><block wx:for="{{likeDatas}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="tui-tab-rank-cent"><view class="img-mask"><image class="img-rink" src="{{item.url}}" mode="aspectFill"></image><block wx:if="{{item.number===0}}"><view class="img-mask-item"><view class="item-text">抢光了</view></view></block></view><view class="tui-pro-tit"><text class="tag-tit">{{item.lableName}}</text><text class="tag-tit-text">{{item.name}}</text><view class="tag-tit2"><view><view class="tag-tit2-price">{{''+item.kg1+'斤装'}}</view><view class="tag-tit2-text"><text class="price1">{{"￥"+(ApproveStatus===1?item.platformClientPrice:'***')+"元"}}</text><text class="price2">{{"/"+(item.isGroup=='2'?'份':'件')}}</text></view></view><block wx:if="{{showLike}}"><image class="tui-shop-car" src="../../static/images/like1.png" mode="aspectFill" data-event-opts="{{[['tap',[['toggleTips',['$0'],[[['likeDatas','',index,'goodsId']]]]]]]}}" bindtap="__e"></image></block><block wx:if="{{!showLike}}"><image class="tui-shop-car" src="../../static/images/like2.png" mode="aspectFill"></image></block></view></view></view></block></view></block><block wx:if="{{showTips}}"><view class="warp"><view class="warp-view"><view class="warp-text1">温馨提示</view><view class="warp-text">取消收藏？</view><view class="warp-flex"><view plain="true" data-event-opts="{{[['tap',[['unShowTips',['$event']]]]]}}" bindtap="__e">取消</view><view class="color-green" data-id="{{11}}" data-event-opts="{{[['tap',[['delLike',['$event']]]]]}}" bindtap="__e">确认</view></view></view></view></block></view>