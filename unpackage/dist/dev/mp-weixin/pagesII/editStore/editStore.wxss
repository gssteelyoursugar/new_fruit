	font-size: 24rpx;
	font-weight: 400;
	color: #666;
	padding-top: 6rpx;
}
.tui-tool-box {
	margin-top: 20rpx;
}
.tui-flex-wrap {
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
	height: auto;
	padding-bottom: 30rpx;
}
.tui-tool-item {
	width: 25%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
