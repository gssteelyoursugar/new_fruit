
button::after {
	border: none;
}
.tui-icon-box {
	color: rgba(51, 51, 51, 1);
	font-size: 28rpx;
	font-weight: 600;
}
.icon-img {
	width: 100rpx;
	height: 100rpx;
	display: block;
	margin: 20rpx 0;
}
.icon-img2 {
	width: 100rpx;
	height: 100rpx;
	background-color: #fff;
}
.icon-img3 {
	background-color: #fff;
	width: 100rpx;
	height: 100rpx;
	padding: 0;
	margin: 20rpx;
}
.tui-box {
	width: 100%;
	background: #fff;
	box-shadow: 0 3rpx 20rpx rgba(183, 183, 183, 0.1);
	border-radius: 10rpx;
	overflow: hidden;
}
.tui-order-box {
	/* height: 208rpx; */
}
.tui-cell-header {
	width: 100%;
	height: 74rpx;
	padding: 0 26rpx;
	box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.tui-cell-title {
	font-size: 30rpx;
	line-height: 30rpx;
	font-weight: 600;
	color: #333;
}
.tui-cell-sub {
	font-size: 26rpx;
	font-weight: 400;
	color: #999;
	padding-right: 28rpx;
}
.tui-order-list {
	width: 100%;
	height: 400rpx;
	padding: 0 30rpx;
	box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.tui-order-item {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.tui-order-text,
.tui-tool-text {
	font-size: 26rpx;
	font-weight: 400;
	color: rgba(188, 188, 188, 1);
	/* padding-top: 22rpx; */
	height: 100rpx;
}
.text-center {
	text-align: center;
	color: rgba(188, 188, 188, 1);
	font-size: 28rpx;
}
.tui-tool-text {
	font-size: 24rpx;
}
.tui-order-icon {
	width: 56rpx;
	height: 56rpx;
	display: block;
}
.tui-assets-box {

	margin-top: 20rpx;
}
.tui-assets-list {
	height: 204rpx;
}
.tui-assets-num {
	font-size: 32rpx;
	font-weight: 500;
	color: #333;
	position: relative;
}
.tui-assets-text {
	font-size: 24rpx;
	font-weight: 400;
	color: #666;
	padding-top: 6rpx;
}
.tui-tool-box {
	margin-top: 20rpx;
}
.tui-flex-wrap {
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
	height: auto;
	padding-bottom: 30rpx;
}
.tui-tool-item {
	width: 25%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	padding-top: 30rpx;
}
.tui-tool-icon {
	width: 64rpx;
	height: 64rpx;
	display: block;
}
.tui-badge-icon {
	width: 66rpx;
	height: 30rpx;
	position: absolute;
	right: 0;
	-webkit-transform: translateX(88%);
	        transform: translateX(88%);
	top: -15rpx;
}

