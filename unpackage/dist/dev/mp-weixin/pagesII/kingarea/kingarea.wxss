	position: absolute;
	width: 100%;
	height: 128rpx;
	line-height: 50rpx;
	left: 0;
	top: 300rpx;
	padding: 0 30rpx;
	box-sizing: border-box;
	/* display: flex; */
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	color: #fff;
}
.tui-info-day{
	font-size: 20rpx;
}
.tui-info-quanguo{
	font-size: 28rpx;
}
.tui-banner-tag {
	position: absolute;
	color: #fff;
	bottom: 30rpx;
	right: 0;
}
.tui-slide-image {
	width: 100%;
	display: block;
}

