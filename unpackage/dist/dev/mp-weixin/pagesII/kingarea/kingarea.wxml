<view class="container"><view class="tui-header-box" style="{{'height:'+(height+'px')+';'+('background:'+('rgba(255,255,255,'+opcity+')')+';')}}"><view class="tui-header" style="{{'padding-top:'+(top+'px')+';'+('opacity:'+(opcity)+';')}}">{{title}}</view><view class="tui-header-icon" style="{{'margin-top:'+(top+'px')+';'}}"><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="tui-icon-box" bindtap="__e"><tui-icon vue-id="1f8dbdc0-1" name="arrowleft" size="{{30}}" color="{{opcity>=1?'#000':'#fff'}}" bind:__l="__l"></tui-icon></view></view></view><view class="tui-banner-swiper"><image class="tui-my-bg" src="{{imageUrl}}" mode="aspectFill"></image></view><view class="tui-rink-sceate"><view class="tui-rank-list"><block wx:for="{{importData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="tui-tab-rank"><view data-event-opts="{{[['tap',[['gotoList',['$0'],[[['importData','',index,'id']]]]]]]}}" class="tui-tab-rank-cent" bindtap="__e"><image class="img-rink" src="{{url+item.url}}" mode="aspectFill"></image><view class="tui-pro-tit"><text class="tag-tit">{{item.lableName}}</text><text class="tag-tit-text">{{item.name}}</text><view class="tag-tit2"><view><view class="tag-tit2-price">{{''+(ApproveStatus===1?item.platformClinetPrice:'***')+'元/件'}}</view><view class="tag-tit2-text">{{''+item.number+'点赞'}}</view></view><image class="tui-shop-car" src="../../static/images/shopcar@2x.png" mode="aspectFill"></image></view></view></view></view></block></view></view></view>