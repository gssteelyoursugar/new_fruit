
.box-title {
	background-color: #fff;
	padding: 0 40rpx;
	position: fixed;
	left: 0;
	right: 0;
	width: 100%;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	height: 224rpx;
	top: 0;
	border-top: 20rpx solid #f5f5f5;
}
.title-time {
	color: #333;
	padding: 30rpx 0 54rpx;
	font-size: 16px;
}
.title-price {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	padding-bottom: 44rpx;
}
.title-price1 {
	font-size: 36rpx;
	color: #333;
}
.title-price1 text:first-child {
	font-size: 28rpx;
}


/* start */
.price1 {
	color: #ff5600;
	font-size: 24rpx;
	font-weight: 600;
}
.price2 {
	color: #b6b6b6;
	font-size: 24rpx;
}
.tui-tab-rank {
	background-color: #fff;
	margin: 20rpx 0;
}
.tui-time-title {
	height: 60rpx;
	line-height: 60rpx;
	border-bottom: 1rpx solid #f5f5f5;
}
.title-time-left {
	margin-left: 40rpx;
	color: #555;
	font-size: 28rpx;
	padding: 14rpx 0;
}
.tui-tab-rank-cent {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	padding: 20rpx;
}
.img-rink {
	width: 180rpx;
	height: 180rpx;
	display: block;
	margin-right: 10rpx;
	/* background: #eee; */
	-webkit-border-radius: 6rpx;
	        border-radius: 6rpx;
}
.tui-pro-tit {
	-webkit-box-flex: 5;
	-webkit-flex: 5;
	        flex: 5;
}
.tag-tit {
	/* 渐变色 */
	background-image: -webkit-gradient(linear, left top, right top, from(#00c94a), to(#00ac3f));
	background-image: -webkit-linear-gradient(left, #00c94a, #00ac3f);
	background-image: linear-gradient(to right, #00c94a, #00ac3f);
	margin-right: 10rpx;
	padding: 0 10rpx;
	-webkit-border-radius: 15rpx 0 15rpx 0;
	        border-radius: 15rpx 0 15rpx 0;
	color: #fff;
	font-size: 24rpx;
}
.tag-tit2 {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.tag-tit2-price {
	color: #555555;
	font-size: 24rpx;
}
.tag-tit2-text {
	color: #ff5600;
	font-size: 36rpx;
	margin: 8rpx 0;
}
.tui-shop-car {
	width: 45rpx;
	height: 48rpx;
	display: block;
}
.tag-tit-text {
	font-size: 28rpx;
	color: #333;
}
.empty-img {
	/* margin-top: 150rpx; */
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.color-text {
	font-size: 32rpx;
	color: rgba(112, 112, 112, 1);
}
.color-border {
	width: 300rpx;
	height: 80rpx;
	line-height: 80rpx;
	border: 1rpx solid rgba(0, 197, 42, 1);
	-webkit-border-radius: 60rpx;
	        border-radius: 60rpx;
	text-align: center;
	color: rgba(0, 197, 42, 1);
	margin-top: 60rpx;
}


/* end */

