<view><block wx:if="{{evaList.length===0}}"><view class="{{['container-img ',(isActive)?'active':'',(hasError)?'text-danger':'']}}"><image src="../../static/images/orderBMG.png" mode="widthFix"></image><text class="color-text">最近没有点赞过商品~</text></view></block><block wx:for="{{evaList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="tui-tab-rank"><view class="tui-time-title"><text class="title-time-left">{{item.time}}</text></view><block wx:for="{{item.list}}" wx:for-item="itm" wx:for-index="idx" wx:key="idx"><view data-event-opts="{{[['tap',[['clickToDetail',['$0'],[[['evaList','',index],['list','',idx,'id']]]]]]]}}" class="tui-tab-rank-cent" bindtap="__e"><image class="img-rink" src="{{itm.url}}" mode="aspectFill"></image><view class="tui-pro-tit"><text class="tag-tit">{{itm.labelName}}</text><text class="tag-tit-text">{{itm.name}}</text><view class="tag-tit2"><view><view class="tag-tit2-price">{{''+itm.specification+''}}</view><view class="tag-tit2-text"><text class="price1"><text style="font-size:20rpx;margin-right:4rpx;">¥</text>{{ApproveStatus===1?itm.platformClientPrice:'***'}}<text style="font-size:20rpx;margin-left:4rpx;">元</text><text class="price2">/件</text></text></view></view><image class="tui-shop-car" src="../../static/images/zan.png" mode="aspectFill" data-event-opts="{{[['tap',[['toggleTips',['$0'],[[['evaList','',index],['list','',idx,'id']]]]]]]}}" catchtap="__e"></image></view></view></view></block></view></block><block wx:if="{{showTips}}"><view class="warp"><view class="warp-view"><view class="warp-text1">温馨提示</view><view class="warp-text">取消点赞？</view><view class="warp-flex"><view plain="true" data-event-opts="{{[['tap',[['unShowTips',['$event']]]]]}}" bindtap="__e">取消</view><view class="color-green" data-id="{{11}}" data-event-opts="{{[['tap',[['cancelPraise',['$event']]]]]}}" bindtap="__e">确认</view></view></view></view></block></view>